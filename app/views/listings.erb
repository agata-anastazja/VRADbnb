<h1>Listings</h1>

<ul id='listings'>
    <% @listings.each do |listing| %>
      <li>
        Name: <%= listing.name %>
        Price: <%= listing.price %>
        Available from: <%= listing.start_date %>
        Available until: <%= listing.end_date %>
        <form method="POST" action="/renter/requests">
          <input type="date">
          <input type="submit" value="Request">
        </form>
      </li>
    <% end %>
</ul>
